<!DOCTYPE html>
<html lang="de">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="Artikel.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="apple-touch-icon" sizes="180x180" href="apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="favicon-16x16.png">
    <link rel="manifest" href="site.webmanifest">
    <title>Manhwa Seiten auf Deutsch</title>

</head>

<body>
  <%- include("navbar") %>


    
      <div class="ArtikelÜ1">
        <h5 id="titel" ><%= Manhwas.Titel %>  </h5>
       
       <section id="bes"> <p><%=Manhwas.Verfasser %></p> <p><%=Manhwas.Datum %></p> </section>
      </div>



      <div  class="ca" style="padding:3%">
     
        
    <% for(let x of Manhwas.Manhwaseiten) { %>
          <div id="liz">
          <div class="card"  style="border-radius: 0px; border: 0px; ">
            <section id="head">    <h1 id="nummer"><%= x.Platzierung %></h1><a  id="a" href="<%=x.Link%>">    <%= x.Titel %>  </a> <div id="star">              <i class="fa-solid fa-star"></i>  <i class="fa-solid fa-star"></i>    <i class="fa-solid fa-star"></i>         </div></section> 
          
             <img  class="card-img-top"    src="<%= x.Cover %>" alt="Cover von der Manhwa Seiten <%= x.Titel %>">
             <div class="card-body">
            
                <p class="card-text"><%= x.Beschreibung %></p>  
             
              
             </div>
           
           <% } %>
            </div>
    </div>
      </div>
    


    <div class="my-5" style="margin-bottom: 0!Important; padding: 0!Important; margin: 0;">

        <footer class="text-center text-white" style="background-color: #f1f1f1;">
        <!-- Grid container -->
        <div class="container pt-4">
          <!-- Section: Social media -->
          <section class="mb-4">
            <!-- Facebook -->
            <a
              class="btn btn-link btn-floating btn-lg text-dark m-1"
              href="#!"
              role="button"
              data-mdb-ripple-color="dark"
              ><i class="fab fa-facebook-f"></i
            ></a>
      
            <!-- Twitter -->
            <a
              class="btn btn-link btn-floating btn-lg text-dark m-1"
              href="https://twitter.com/ManhwaDEU"
              role="button"
              data-mdb-ripple-color="dark"
              ><i class="fab fa-twitter"></i
            ></a>
      
            
            <!-- Instagram -->
            <a
              class="btn btn-link btn-floating btn-lg text-dark m-1"
              href="https://www.instagram.com/manhwadeu/?next=%2F"
              role="button"
              data-mdb-ripple-color="dark"
              ><i class="fab fa-instagram"></i
            ></a>
      
            
           
          </section>
          <!-- Section: Social media -->
        </div>
        <!-- Grid container -->
      
        <!-- Copyright -->
        <div class="text-center text-dark p-3" style="background-color: rgba(0, 0, 0, 0.2);">
          ©email:
          <a class="text-dark" >manhwadeu@outlook.de</a>
        </div>
        <!-- Copyright -->
      </footer>
        
      </div>
      <!-- End of .container -->

      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>